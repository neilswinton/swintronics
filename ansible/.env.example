# Copy this file to ansible/.env and fill in your values.
# Source it before running playbooks:  source ansible/.env

# Infisical Machine Identity (Universal Auth)
# Create at: https://app.infisical.com → Project → Access Control → Machine Identities
export INFISICAL_CLIENT_ID=
export INFISICAL_CLIENT_SECRET=

# Infisical project UUID (shown in Project Settings → General)
export INFISICAL_PROJECT_ID=
